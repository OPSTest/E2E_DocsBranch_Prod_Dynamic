{
  "build": {
    "content": [
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "netstandard-1.0-src",
        "version": "netstandard-1.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "netstandard-1.1-src",
        "version": "netstandard-1.1",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "reference-1.2-src",
        "version": "netstandard-1.2",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "files": [ "**/*.yml", "**/*.md" ],
        "src": "hubpage-1.3-src",
        "version": "netstandard-1.3",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "files": [ "**/*.yml", "**/*.md" ],
        "src": "reference-src"
      },
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "hubpage-src"
      },
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "E2E_DocsBranch_Prod_Dynamic"
      },
      {
        "files": ["*.md", "*.yml"]
      }
    ],
    "resource": [
        {
          "files": ["**/**.bmp", "**/**.gif", "**/**.ico", "**/**.jpg", "**/**.jpeg", "**/**.png"],
          "exclude": [
            "**/obj/**",
            "_themes/**"
          ]
        },
        {
          "files": ["**/**.bmp", "**/**.gif", "**/**.ico", "**/**.jpg", "**/**.jpeg", "**/**.png"],
          "exclude": [
            "**/obj/**",
            "_themes/**"
          ],
         "src": "netstandard-1.1-src",
         "version": "netstandard-1.1"
        }
    ],
    "versions": {
      "netstandard-1.0": {
        "dest": "netstandard-1.0-desk"
      },
      "netstandard-1.1": {
        "dest": "netstandard-1.1-desk"
      },
      "netstandard-1.2": {
        "dest": "netstandard-1.2-desk"
      },
      "netstandard-1.3": {
        "dest": "netstandard-1.3-desk"
      }
    },
    "dest": "testMultipleVersion"
  }
}
